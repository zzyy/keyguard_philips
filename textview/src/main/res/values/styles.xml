<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="UnreadNumAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/mtk_ic_newevents_numberindication</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">20.0dip</item>
        <item name="android:includeFontPadding">false</item>
    </style>
    <style name="label">
        <item name="android:textSize">@dimen/label_font_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <!--<item name="android:textUnderlineThickness">sans-serif-condensed</item>-->
    </style>
    <style name="header">
        <item name="android:textSize">@dimen/header_font_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <!--<item name="android:textUnderlineThickness">sans-serif-condensed</item>-->
    </style>
    <style name="header_not_caps" parent="@style/header">
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="big_bold">
        <item name="android:textSize">@dimen/big_font_size</item>
        <item name="android:textStyle">bold</item>
        <!--<item name="android:textUnderlineThickness">sans-serif</item>-->
    </style>
    <style name="widget_big_bold" parent="@style/big_bold">
        <item name="android:textSize">@dimen/widget_big_font_size</item>
    </style>
    <style name="DismissPadKey" parent="@android:style/Widget.Button">
        <item name="android:textSize">20.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:singleLine">true</item>
        <!--<item name="android:textUnderlineThickness">sans-serif</item>-->
    </style>
    <style name="Widget.Button.NumPadKey" parent="@android:style/Widget.Button">
        <item name="android:textSize">34.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:singleLine">true</item>
        <!--<item name="android:textUnderlineThickness">sans-serif</item>-->
    </style>
    <style name="TextAppearance.NumPadKey" parent="@android:style/TextAppearance">
        <item name="android:textSize">34.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
        <!--<item name="android:textUnderlineThickness">sans-serif</item>-->
    </style>
    <style name="TextAppearance.NumPadKey.Klondike" parent="@style/TextAppearance.NumPadKey">
        <item name="android:textSize">20.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#80ffffff</item>
        <!--<item name="android:textUnderlineThickness">sans-serif-condensed</item>-->
    </style>
    <style name="Animation.LockScreen" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/lock_screen_enter</item>
        <item name="android:windowExitAnimation">@anim/lock_screen_exit</item>
    </style>
    <style name="widget_label">
        <item name="android:textSize">@dimen/widget_label_font_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <!--<item name="android:textUnderlineThickness">sans-serif-condensed</item>-->
    </style>
    <style name="big_thin">
        <item name="android:textSize">@dimen/big_font_size</item>
        <!--<item name="android:textUnderlineThickness">sans-serif-thin</item>-->
    </style>
    <style name="widget_big_thin" parent="@style/big_thin">
        <item name="android:textSize">@dimen/widget_big_font_size</item>
    </style>
    <style name="Widget.TransportControl.SeekBar" parent="@android:style/Widget.Holo.SeekBar">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">13.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_light</item>
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13.0dip</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo</item>
        <item name="android:thumbOffset">16.0dip</item>
        <!--<item name="android:alertDialogCenterButtons">16.0dip</item>-->
        <!--<item name="android:panelMenuIsCompact">16.0dip</item>-->
        <!--<item name="android:searchViewVoiceIcon">true</item>-->
    </style>
    <style name="widget_name">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="calendar_time">
        <item name="android:textSize">65.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="calendar_ampm">
        <item name="android:textSize">15.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:singleLine">true</item>
    </style>
</resources>